.login-subhead
	margin-top: 20px
	font-size: 11pt
	font-weight: bold
	color: gray
#login
	display: table
	margin: 0 auto
#login-left
	display: table-cell
	margin: 50px
#login-right
	display: table-cell
	padding-left: 50px
.login-submit-wrap
	text-align: right
.login-submit
	margin-top: 15px
	font-weight: bold
	padding: 2px
	padding-left: 8px
	padding-right: 8px
	font-size: 12pt
	background: #0B5FA5
	color: #FFFFFF
	border: none
	outline: none
	opacity: 0.6
	cursor: pointer
.login-submit:hover
	opacity:1.0
.login-input
	border-style: solid
	border-color: gray
	border-width: 1px
	padding-left: 2px
	margin-top: 2px
.login-header
	text-align: center
	font-weight: bold
	margin-bottom: 15px
.login-aux
	text-align: right
	color: #0B5FA5
	font-size: 10pt
	margin: 4px
	margin-top: 10px
#login-aux-remember	
	text-align: right
	margin: 0
#login-aux-recover
	text-align: right
	margin: 0
